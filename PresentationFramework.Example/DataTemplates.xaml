<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:shellExample="clr-namespace:WpfExampleApplication.PresentationFramework.Example.ShellExample">

    <DataTemplate DataType="{x:Type shellExample:ShellDisplayViewModel}">
        <shellExample:ShellDisplay />
    </DataTemplate>

    <DataTemplate DataType="{x:Type shellExample:InputDialogDisplayViewModel}">
        <shellExample:InputDialogDisplay/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type shellExample:MessageDialogDisplayViewModel}">
        <shellExample:MessageDialogDisplay/>
    </DataTemplate>

</ResourceDictionary>